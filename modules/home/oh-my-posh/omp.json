{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "debian": "\uf306",
            "windows": "\uf17a"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#d0b1fc",
          "background": "#584d80",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{if .Root}}{{ .UserName }}üíÄ{{ .HostName }} {{else}}{{ .UserName }}@{{ .HostName }} {{end}} ",
          "foreground": "#d0b1fc",
          "background": "#584d80",
          "type": "session",
          "style": "diamond",
          "foreground_templates": [
            "{{if .Root}}#ff385d{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uea7c",
            "folder_separator_icon": " / ",
            "home_icon": "\uf015 ",
            "mapped_locations": {},
            "max_depth": 1,
            "style": "agnoster_short"
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Path }} ",
          "foreground": "#584d80",
          "background": "#d0b1fc",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "commit_icon": "\uf417 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ if( .UpstreamIcon ) }}{{ url .UpstreamIcon .UpstreamURL }}{{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git",
          "style": "diamond",
	  "foreground": "transparent",
	  "background": "#927E86",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#E69AB8{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#E6A59A{{ end }}",
            "{{ if gt .Ahead 0 }}#FFD4CC{{{ end }}",
            "{{ if gt .Behind 0 }}#C9AAF0{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": "<transparent> \uf12a</> {{ .Meaning }} ",
          "background": "#910000",
          "type": "status",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0bc",
          "template": " \udb86\udd9f {{ .FormattedMs }}",
          "foreground": "#503A6B",
          "background": "#F0B4AA",
          "type": "executiontime",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \udb85\udc4e {{ .CurrentDate | date .Format }} ",
          "foreground": "#F0B4AA",
          "background": "#503A6B",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\n‚ùØ ",
          "foreground": "#fff",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ]
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
